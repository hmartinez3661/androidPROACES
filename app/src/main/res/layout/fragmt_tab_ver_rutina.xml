<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f2f2f2"
    android:orientation="vertical"
    tools:context=".ui.Rutinas.FragmtTabVerRutina">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/linearLayout0"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/style_titulos"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/dividerX">

                <TextView
                    android:id="@+id/tvNumRut"
                    android:layout_width="75dp"
                    android:layout_height="wrap_content"
                    android:text="@string/headNumRut"
                    android:layout_marginTop="3dp"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="16sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#515053"/>

                <TextView
                    android:id="@+id/tvIndicaciones"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:text="@string/informRut"
                    android:gravity="center"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="15dp"
                    android:layout_gravity="center"
                    android:textAlignment="center"
                    android:textSize="13sp" />

            </LinearLayout>

            <View
                android:id="@+id/dividerX"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="1dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout0"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout"/>

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/dividerX" >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.20"
                    android:textColor="@color/black"
                    android:text="@string/etMaquina"
                    android:textStyle="bold"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/etNombreEquip"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.80"
                    android:layout_marginEnd="5dp"
                    android:autofillHints=""
                    android:enabled="false"
                    android:textColor="@color/black"
                    android:inputType="none"
                    android:textSize="15sp" />

            </LinearLayout>

            <EditText
                android:id="@+id/etTrabajoRut"
                android:layout_width="0dp"
                android:layout_height="75dp"
                android:gravity="top"
                android:inputType="textMultiLine"
                android:textSize="15sp"
                android:enabled="false"
                android:textColor="@color/black"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:background="@drawable/style_bordes_edittex1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linearLayout"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.4" />

            <TextView
                android:id="@+id/tvEjector"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:text="@string/ejectRutina"
                android:layout_marginStart="16dp"
                android:textAlignment="textEnd"
                android:textColor="@color/black"
                android:layout_marginTop="10dp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline1"
                app:layout_constraintTop_toBottomOf="@+id/etTrabajoRut"/>

            <TextView
                android:id="@+id/tvFrecuenc"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:textAlignment="textEnd"
                android:textSize="15sp"
                android:text="@string/frecuRut"
                android:textStyle="bold"
                android:layout_marginTop="5dp"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@+id/tvEjector"
                app:layout_constraintEnd_toStartOf="@id/guideline1"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/tvPersEstim"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:textAlignment="textEnd"
                android:textSize="15sp"
                android:text="@string/persEstimado"
                android:textStyle="bold"
                android:layout_marginTop="5dp"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@+id/tvFrecuenc"
                app:layout_constraintEnd_toStartOf="@id/guideline1"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/tvTpoEstim"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:textAlignment="textEnd"
                android:textSize="15sp"
                android:text="@string/tmpoEsitmado"
                android:textStyle="bold"
                android:layout_marginTop="5dp"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@+id/tvPersEstim"
                app:layout_constraintEnd_toStartOf="@id/guideline1"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/tvEstEquipo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:textAlignment="textEnd"
                android:textSize="15sp"
                android:text="@string/tvEstadoEquip"
                android:textStyle="bold"
                android:layout_marginTop="5dp"
                android:textColor="@color/black"
                app:layout_constraintTop_toBottomOf="@+id/tvTpoEstim"
                app:layout_constraintEnd_toStartOf="@id/guideline1"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/tvEjecutoRutDt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="15dp"
                android:textStyle="bold"
                android:textSize="15sp"
                android:layout_marginTop="10dp"
                android:text="@string/lrFile01"
                app:layout_constraintTop_toBottomOf="@+id/etTrabajoRut"
                app:layout_constraintStart_toStartOf="@id/guideline1"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/tvFrecucRutDt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="15dp"
                android:textStyle="bold"
                android:textSize="15sp"
                android:layout_marginTop="5dp"
                android:text="@string/lrFile01"
                app:layout_constraintTop_toBottomOf="@+id/tvEjecutoRutDt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline1"/>

            <TextView
                android:id="@+id/tvtPersEstimDt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="15dp"
                android:textStyle="bold"
                android:textSize="15sp"
                android:layout_marginTop="5dp"
                android:text="@string/lrFile01"
                app:layout_constraintTop_toBottomOf="@+id/tvFrecucRutDt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline1"/>

            <TextView
                android:id="@+id/tvTpoEstimDt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="15dp"
                android:textStyle="bold"
                android:textSize="15sp"
                android:layout_marginTop="5dp"
                android:text="@string/lrFile01"
                app:layout_constraintTop_toBottomOf="@+id/tvtPersEstimDt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline1"/>

            <TextView
                android:id="@+id/tvEstEquip"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="15dp"
                android:textStyle="bold"
                android:textSize="15sp"
                android:text="@string/lrFile01"
                android:layout_marginTop="5dp"
                app:layout_constraintTop_toBottomOf="@+id/tvTpoEstimDt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/guideline1"/>

            <View
                android:id="@+id/divider3"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvEstEquip"/>

            <TextView
                android:id="@+id/tvMedidasSegur"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvMedidasSegur"
                android:textStyle="bold"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:textSize="15sp"
                android:layout_marginTop="10dp"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider3"/>

            <EditText
                android:id="@+id/etMedidadSegur"
                android:layout_width="0dp"
                android:layout_height="75dp"
                android:gravity="top"
                android:inputType="textMultiLine"
                android:textSize="15sp"
                android:enabled="false"
                android:textColor="@color/black"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/style_bordes_edittex1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvMedidasSegur"/>

            <TextView
                android:id="@+id/tvCoordPrev"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvCoodinPrev"
                android:textStyle="bold"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:textSize="15sp"
                android:layout_marginTop="10dp"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etMedidadSegur"/>

            <EditText
                android:id="@+id/etCoodinPrev"
                android:layout_width="0dp"
                android:layout_height="75dp"
                android:gravity="top"
                android:inputType="textMultiLine"
                android:textSize="15sp"
                android:enabled="false"
                android:textColor="@color/black"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/style_bordes_edittex1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvCoordPrev"/>

            <TextView
                android:id="@+id/tvPrepPrev"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvPrepPrev"
                android:textStyle="bold"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:textSize="15sp"
                android:layout_marginTop="10dp"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etCoodinPrev"/>

            <EditText
                android:id="@+id/etPrepPrev"
                android:layout_width="0dp"
                android:layout_height="75dp"
                android:gravity="top"
                android:inputType="textMultiLine"
                android:textSize="15sp"
                android:enabled="false"
                android:textColor="@color/black"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/style_bordes_edittex1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvPrepPrev"/>

            <View
                android:id="@+id/divider4"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/etPrepPrev"/>

            <ProgressBar
                android:id="@+id/progresBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:indeterminate="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


</FrameLayout>