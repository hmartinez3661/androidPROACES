<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#f2f2f2"
    tools:context=".ui.listadoOTs.FragmentTabVerRepEje">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/linearLayout0"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/style_titulos"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" >

                <TextView
                    android:id="@+id/tvNumOT"
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="@string/tvNumOT"
                    android:textColor="@color/black"
                    android:layout_marginTop="10dp"
                    android:textStyle="bold"
                    android:textSize="16sp" />

                <View
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#474649"/>

                <TextView
                    android:id="@+id/tvIndicacionesX"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:text="@string/tvEncabRevRepEjec"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    android:layout_gravity="center"
                    android:textAlignment="center"
                    android:textSize="15sp" />

            </LinearLayout>

            <View
                android:id="@+id/divider1"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginTop="1dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout0"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.4" />

            <TextView
                android:id="@+id/tvNombreApellido"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvfechaEjecuc"
                android:textStyle="bold"
                android:layout_marginStart="16dp"
                android:layout_marginTop="15dp"
                android:textSize="15sp"
                android:gravity="end"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/divider1"/>

            <TextView
                android:id="@+id/etNombrApellido1"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="5dp"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/style_bordes_edittex2"
                app:layout_constraintStart_toStartOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/divider1"/>

            <TextView
                android:id="@+id/tvEmail"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvHrsParoProd"
                android:textStyle="bold"
                android:layout_marginStart="16dp"
                android:layout_marginTop="15dp"
                android:textSize="15sp"
                android:gravity="end"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/tvNombreApellido"/>

            <EditText
                android:id="@+id/etHrsProdStop"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:background="@drawable/style_bordes_edittex3"
                android:selectAllOnFocus="true"
                android:inputType="numberDecimal"
                android:gravity="center"
                android:layout_marginTop="5dp"
                android:layout_marginStart="10dp"
                android:text="@string/tvValor00"
                android:enabled="false"
                app:layout_constraintStart_toEndOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/etNombrApellido1"/>

            <TextView
                android:id="@+id/tvHrsLabor"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvHrsTrabajo"
                android:textStyle="bold"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:textSize="15sp"
                android:gravity="end"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/tvEmail"/>

            <EditText
                android:id="@+id/etHrsLabor"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:background="@drawable/style_bordes_edittex3"
                android:selectAllOnFocus="true"
                android:inputType="numberDecimal"
                android:gravity="center"
                android:layout_marginTop="5dp"
                android:layout_marginStart="10dp"
                android:text="@string/tvValor05"
                android:enabled="false"
                app:layout_constraintStart_toEndOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/etHrsProdStop"/>

            <TextView
                android:id="@+id/tvCalidadTrab"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvCalidadTrab"
                android:textStyle="bold"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:textSize="15sp"
                android:gravity="end"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/tvHrsLabor"/>

            <EditText
                android:id="@+id/etCalidadTrab"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:background="@drawable/style_bordes_edittex3"
                android:selectAllOnFocus="true"
                android:inputType="numberDecimal"
                android:gravity="center"
                android:layout_marginTop="5dp"
                android:layout_marginStart="10dp"
                android:text="@string/tvValor60"
                android:enabled="false"
                app:layout_constraintStart_toEndOf="@+id/guideline3"
                app:layout_constraintTop_toBottomOf="@+id/etHrsLabor"/>

            <View
                android:id="@+id/divider2B"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginTop="10dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etCalidadTrab"/>

            <TextView
                android:id="@+id/tvIndicRepEjec"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvIndRepteHist"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:textSize="15sp"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider2B"/>

            <EditText
                android:id="@+id/etReportEjec"
                android:layout_width="0dp"
                android:layout_height="75dp"
                android:gravity="top"
                android:hint="@string/etReportEjec"
                android:inputType="textMultiLine"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:enabled="false"
                android:background="@drawable/style_bordes_edittex1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvIndicRepEjec"/>

            <View
                android:id="@+id/divider1C"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginTop="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etReportEjec"/>

            <TextView
                android:id="@+id/tvNombFalla"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvNombFalla"
                android:textStyle="bold"
                android:layout_marginStart="15dp"
                android:layout_marginTop="20dp"
                android:textSize="15sp"
                android:gravity="end"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline1"
                app:layout_constraintTop_toBottomOf="@+id/divider1C"/>

            <TextView
                android:id="@+id/tvNombrFalla"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="10dp"
                android:background="@drawable/style_bordes_edittex3"
                android:hint="@string/tvNombrFalla"
                android:padding="7dp"
                android:textColor="@color/black"
                android:textSize="15sp"
                android:enabled="false"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/guideline1"
                app:layout_constraintTop_toBottomOf="@id/divider1C" />

            <TextView
                android:id="@+id/tvSupervOT"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvSupervOT"
                android:textStyle="bold"
                android:layout_marginStart="15dp"
                android:layout_marginTop="20dp"
                android:textSize="15sp"
                android:gravity="end"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline1"
                app:layout_constraintTop_toBottomOf="@+id/tvNombFalla"/>

            <TextView
                android:id="@+id/tvNombrSupOT"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="top"
                android:hint="@string/tvNombrSupOT"
                android:padding="7dp"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:enabled="false"
                android:background="@drawable/style_bordes_edittex3"
                app:layout_constraintStart_toStartOf="@+id/guideline1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvNombrFalla"/>

            <TextView
                android:id="@+id/tvPersRecivTrab"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvPersRecivTrab"
                android:textStyle="bold"
                android:layout_marginStart="15dp"
                android:layout_marginTop="15dp"
                android:textSize="15sp"
                android:gravity="end"
                android:textAlignment="gravity"
                android:textColor="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline1"
                app:layout_constraintTop_toBottomOf="@+id/tvSupervOT"/>

            <EditText
                android:id="@+id/etNombRecivTrab"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="top"
                android:hint="@string/etNombRecivTrab"
                android:padding="7dp"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:layout_marginTop="5dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:enabled="false"
                android:background="@drawable/style_bordes_edittex3"
                app:layout_constraintStart_toStartOf="@+id/guideline1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvNombrSupOT"/>

            <View
                android:id="@+id/divider2"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginTop="16dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/etNombRecivTrab"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.67" />

            <TextView
                android:id="@+id/tvIndicPersonal"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvIndPersEjec"
                android:layout_marginTop="15dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:textSize="14sp"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:background="@drawable/style_tbls_header1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider2"/>

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.5" />

            <TableLayout
                android:id="@+id/tblPersTecn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvIndicPersonal">

                <TableRow
                    android:id="@+id/tblHeadRow"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="#576166" >

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="wrap_content"
                        android:text="@string/tvNum"
                        android:textAlignment="center"
                        android:background="@drawable/style_tbls_header1"
                        android:textColor="@color/white"/>

                    <TextView
                        android:layout_width="220dp"
                        android:layout_height="wrap_content"
                        android:text="@string/tblHeadPersonal"
                        android:background="@drawable/style_tbls_header1"
                        android:textColor="@color/white"/>

                    <TextView
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:text="@string/tvHrs"
                        android:textAlignment="center"
                        android:background="@drawable/style_tbls_header1"
                        android:textColor="@color/white"/>
                </TableRow>
            </TableLayout>

            <View
                android:id="@+id/divider3"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_marginTop="20dp"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginBottom="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tblPersTecn"/>

            <!-- ***************** Inicio Repuestos -->

            <TextView
                android:id="@+id/tvIndicRepuest"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvIndVerRepuests"
                android:layout_marginStart="10dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="10dp"
                android:textAlignment="center"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:background="@drawable/style_tbls_header2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider3"/>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvRepuestos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:cardBackgroundColor="#ABF1AE"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvIndicRepuest" >

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="10dp">

                    <TableLayout
                        android:id="@+id/tblRepuestos"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginEnd="10dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TableRow
                            android:id="@+id/tblHeadRowRep"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="#93B894">

                            <TextView
                                android:layout_width="40dp"
                                android:layout_height="wrap_content"
                                android:text="@string/tvNum"
                                android:textAlignment="center"
                                android:background="@drawable/style_tbls_header2"
                                android:textStyle="bold"/>

                            <TextView
                                android:layout_width="210dp"
                                android:layout_height="wrap_content"
                                android:text="@string/nombreRepto"
                                android:background="@drawable/style_tbls_header2"
                                android:textStyle="bold"/>

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:text="@string/tvEncabesCosto"
                                android:textAlignment="center"
                                android:background="@drawable/style_tbls_header2"
                                android:textStyle="bold"/>

                        </TableRow>
                    </TableLayout>

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.67" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline3B"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.5" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <!-- ***************** Fin de Repuestos -->

            <View
                android:id="@+id/divider4"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_margin="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cvRepuestos"/>

            <!-- ************************* Inicio Serv. Externos -->

            <TextView
                android:id="@+id/tvIndicServExt"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/tvIndVerServExt"
                android:layout_marginBottom="5dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="10dp"
                android:textAlignment="center"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:background="@drawable/style_tbls_header3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider4"/>

            <androidx.cardview.widget.CardView
                android:id="@+id/cvServExt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:cardBackgroundColor="#F7ED95"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvIndicServExt" >

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="10dp">

                    <TableLayout
                        android:id="@+id/tblServExt"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginEnd="10dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <TableRow
                            android:id="@+id/tblHeadRowSExt"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:background="#D0CB9B" >

                            <TextView
                                android:layout_width="40dp"
                                android:layout_height="wrap_content"
                                android:text="@string/tvNum"
                                android:textAlignment="center"
                                android:background="@drawable/style_tbls_header3"
                                android:textStyle="bold"/>

                            <TextView
                                android:layout_width="195dp"
                                android:layout_height="wrap_content"
                                android:text="@string/tblHeadNombServ"
                                android:background="@drawable/style_tbls_header3"
                                android:textStyle="bold"/>

                            <TextView
                                android:layout_width="70dp"
                                android:layout_height="wrap_content"
                                android:text="@string/tvEncabesCosto"
                                android:textAlignment="center"
                                android:background="@drawable/style_tbls_header3"
                                android:textStyle="bold"/>

                        </TableRow>
                    </TableLayout>

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline2C"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.67" />

                    <androidx.constraintlayout.widget.Guideline
                        android:id="@+id/guideline5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:layout_constraintGuide_percent="0.5" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <!-- ************************* Fin Serv. Externos -->

            <View
                android:id="@+id/divider5"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:background="#9E9CA2"
                android:layout_margin="15dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cvServExt"/>

            <TextView
                android:id="@+id/tvIndicFotosCierre"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:layout_margin="10dp"
                android:textStyle="bold"
                android:text="@string/tvIndVerFotosTrab"
                android:textColor="#1F6AA6"
                android:textAlignment="center"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider5"/>

